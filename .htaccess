<ifModule mod_expires.c>

# Compress compressible fonts
AddOutputFilterByType DEFLATE application/x-font-ttf application/x-font-opentype image/svg+xml

ExpiresActive On
ExpiresDefault "access plus 5 seconds"

# Cache Images
ExpiresByType image/header-img.jpg "access plus 2592000 seconds"
ExpiresByType image/settings.svg "access plus 2592000 seconds"
ExpiresByType image/mail.svg "access plus 2592000 seconds"
ExpiresByType image/chat.svg "access plus 2592000 seconds"
ExpiresByType image/forecast.svg "access plus 2592000 seconds"
ExpiresByType image/favicon.ico "access plus 2592000 seconds"

# Cache other content types (Flash, CSS, JS, HTML, XML)
ExpiresByType application/x-shockwave-flash "access plus 2592000 seconds"
ExpiresByType text/css "access plus 604800 seconds"
ExpiresByType text/javascript "access plus 2592000 seconds"
ExpiresByType application/javascript "access plus 2592000 seconds"
ExpiresByType application/x-javascript "access plus 2592000 seconds"
ExpiresByType text/html "access plus 600 seconds"
ExpiresByType application/xhtml+xml "access plus 600 seconds"

</ifModule>